<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for(@user) do |f| %>
        <%= f.label :name %>
        <%=f.text_field :name %>
        
        <%= f.label :school %>
        <%= f.text_field :school %>
        
        <%= f.submit "I'd like to know more", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>